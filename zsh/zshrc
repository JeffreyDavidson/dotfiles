# Set variables
export NULLCMD=bat
export DOTFILES="$HOME/dotfiles"

# Change ZSH Options

# Add Locations to $path Array
typeset -U path

path=(
  "$N_PREFIX/bin"
  $path
  "/Applications/Visual Studio Code.app/Contents/Resources/app/bin"
)

eval "$(starship init zsh)"

source "$HOME/.shell/.aliases"
source "$HOME/.shell/.functions"

