{
    "Query Selector": {
        "prefix": "ds",
        "body": [
            "document.querySelector('$1')"
        ]
    },

    "Console Log": {
        "prefix": "con",
        "body": [
            "console.log($1)"
        ]
    },

    "Function": {
        "prefix": "fun",
        "body": [
            "function $1($2) {",
            "    $3",
            "}"
        ]
    }
}
