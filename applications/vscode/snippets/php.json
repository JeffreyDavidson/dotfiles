{
    "Method": {
        "prefix": "met",
        "body": [
            "public function $1($2)",
            "{",
            "   $3",
            "}"
        ]
    },

    "Test Method": {
        "prefix": "test",
        "body": [
            "/** @test */",
            "public function $1()",
            "{",
            "   $3",
            "}"
        ]
    },

    "Tinker": {
        "prefix": "tinker",
        "body": [
            "eval(\\Psy\\sh());"
        ]
    },

    "Computed Property": {
        "prefix": "comp",
        "body": [
            "#[Computed]",
            "public function $1()",
            "{",
            "    $2",
            "}"
        ]
    },

    "Livewire Url Property": {
        "prefix": "url",
        "body": [
            "#[Url$3]",
            "public $1 = $2;",
        ]
    },

    "Livewire Mount Method": {
        "prefix": "mount",
        "body": [
            "public function mount()",
            "{",
            "    $1",
            "}"
        ]
    },

    "Private Method": {
        "prefix": "pmet",
        "body": [
            "private function ${1}(${2})",
            "{",
            "    ${0}",
            "}"
        ],
        "description": "PHP private function"
    },
}
